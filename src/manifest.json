{
    "manifest_version": 3,
    "name": "Recording Playback Tool for Zoom",
    "description": "Enhance your Zoom recording playback experience.",
    "version": "1.5",
    "content_scripts": [
        {
            "matches": ["*://*.zoom.us/rec/play/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
